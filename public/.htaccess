<IfModule mod_rewrite.c>
RewriteEngine On
RewriteBase /neville/public/
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule ^(.*)$ index.php?route=$1 [L]
RewriteRule ^([^?]*) index.php?route=$1 [L,QSA]



#RewriteBase /neville/public/
#RewriteRule ^index\.php$ - [L]
#RewriteCond %{REQUEST_FILENAME} !-f
#RewriteCond %{REQUEST_FILENAME} !-d
#RewriteRule . /index.php [L]
</IfModule>